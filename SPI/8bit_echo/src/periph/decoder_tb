#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/va_math.vpi";
S_0x558973ba30 .scope module, "decoder_tb" "decoder_tb" 2 3;
 .timescale -9 -12;
v0x558978a0b0_0 .var "byte_ready", 0 0;
v0x558978a180_0 .var "command_byte", 7 0;
v0x558978a250_0 .net "toggle_mask", 63 0, v0x5589789f70_0;  1 drivers
S_0x558973bbc0 .scope task, "test_command" "test_command" 2 20, 2 20 0, S_0x558973ba30;
 .timescale -9 -12;
v0x558973bda0_0 .var "cmd", 7 0;
TD_decoder_tb.test_command ;
    %load/vec4 v0x558973bda0_0;
    %store/vec4 v0x558978a180_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558978a0b0_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558978a0b0_0, 0, 1;
    %delay 1000, 0;
    %vpi_call 2 27 "$display", "Command: 0x%h | Index: %0d | Toggle Mask: %b", v0x558973bda0_0, &PV<v0x558973bda0_0, 0, 6>, v0x558978a250_0 {0 0 0};
    %end;
S_0x5589789b20 .scope module, "uut" "decoder" 2 13, 3 1 0, S_0x558973ba30;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "byte_ready";
    .port_info 1 /INPUT 8 "command_byte";
    .port_info 2 /OUTPUT 64 "toggle_mask";
v0x5589789db0_0 .net "byte_ready", 0 0, v0x558978a0b0_0;  1 drivers
v0x5589789e90_0 .net "command_byte", 7 0, v0x558978a180_0;  1 drivers
v0x5589789f70_0 .var "toggle_mask", 63 0;
E_0x5589778620 .event posedge, v0x5589789db0_0;
    .scope S_0x5589789b20;
T_1 ;
    %wait E_0x5589778620;
    %pushi/vec4 1, 0, 64;
    %load/vec4 v0x5589789e90_0;
    %parti/s 6, 0, 2;
    %ix/vec4 4;
    %shiftl 4;
    %assign/vec4 v0x5589789f70_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x558973ba30;
T_2 ;
    %vpi_call 2 32 "$display", "Starting decoder testbench..." {0 0 0};
    %vpi_call 2 33 "$dumpfile", "decoder_tb.vcd" {0 0 0};
    %vpi_call 2 34 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x558973ba30 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558978a0b0_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x558978a180_0, 0, 8;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x558973bda0_0, 0, 8;
    %fork TD_decoder_tb.test_command, S_0x558973bbc0;
    %join;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x558973bda0_0, 0, 8;
    %fork TD_decoder_tb.test_command, S_0x558973bbc0;
    %join;
    %pushi/vec4 63, 0, 8;
    %store/vec4 v0x558973bda0_0, 0, 8;
    %fork TD_decoder_tb.test_command, S_0x558973bbc0;
    %join;
    %pushi/vec4 165, 0, 8;
    %store/vec4 v0x558973bda0_0, 0, 8;
    %fork TD_decoder_tb.test_command, S_0x558973bbc0;
    %join;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x558973bda0_0, 0, 8;
    %fork TD_decoder_tb.test_command, S_0x558973bbc0;
    %join;
    %delay 10000, 0;
    %vpi_call 2 47 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "decoder_tb.v";
    "decoder.v";
