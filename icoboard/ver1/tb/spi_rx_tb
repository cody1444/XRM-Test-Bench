#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/va_math.vpi";
S_0x556a1239d0 .scope module, "spi_rx_tb" "spi_rx_tb" 2 3;
 .timescale -9 -12;
P_0x556a123b60 .param/l "WIDTH" 0 2 6, +C4<00000000000000000000000000001000>;
v0x556a177b50_0 .net "byte_ready", 0 0, v0x556a177670_0;  1 drivers
v0x556a177c10_0 .net "command_byte", 7 0, v0x556a177730_0;  1 drivers
v0x556a177ce0_0 .var "cs", 0 0;
v0x556a177de0_0 .var "mosi", 0 0;
v0x556a177eb0_0 .var "sclk", 0 0;
S_0x556a123c00 .scope task, "send_spi_byte" "send_spi_byte" 2 27, 2 27 0, S_0x556a1239d0;
 .timescale -9 -12;
v0x556a123de0_0 .var "data", 7 0;
v0x556a1770d0_0 .var/i "i", 31 0;
TD_spi_rx_tb.send_spi_byte ;
    %delay 5000, 0;
    %pushi/vec4 7, 0, 32;
    %store/vec4 v0x556a1770d0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x556a1770d0_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556a177eb0_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x556a123de0_0;
    %load/vec4 v0x556a1770d0_0;
    %part/s 1;
    %store/vec4 v0x556a177de0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556a177eb0_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x556a1770d0_0;
    %subi 1, 0, 32;
    %store/vec4 v0x556a1770d0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556a177eb0_0, 0, 1;
    %delay 20000, 0;
    %end;
S_0x556a1771b0 .scope module, "uut" "spi_rx" 2 18, 3 1 0, S_0x556a1239d0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "sclk";
    .port_info 1 /INPUT 1 "cs";
    .port_info 2 /INPUT 1 "mosi";
    .port_info 3 /OUTPUT 8 "command_byte";
    .port_info 4 /OUTPUT 1 "byte_ready";
P_0x556a1773b0 .param/l "WIDTH" 0 3 2, +C4<00000000000000000000000000001000>;
v0x556a177570_0 .var "bit_count", 2 0;
v0x556a177670_0 .var "byte_ready", 0 0;
v0x556a177730_0 .var "command_byte", 7 0;
v0x556a177820_0 .net "cs", 0 0, v0x556a177ce0_0;  1 drivers
v0x556a1778e0_0 .net "mosi", 0 0, v0x556a177de0_0;  1 drivers
v0x556a1779f0_0 .net "sclk", 0 0, v0x556a177eb0_0;  1 drivers
E_0x556a162300 .event negedge, v0x556a177820_0;
E_0x556a161b50 .event negedge, v0x556a1779f0_0;
    .scope S_0x556a1771b0;
T_1 ;
    %wait E_0x556a161b50;
    %load/vec4 v0x556a177730_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x556a1778e0_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x556a177730_0, 0;
    %load/vec4 v0x556a177570_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x556a177570_0, 0;
    %load/vec4 v0x556a177570_0;
    %pad/u 32;
    %cmpi/e 7, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x556a177670_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x556a177670_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x556a1771b0;
T_2 ;
    %wait E_0x556a162300;
    %load/vec4 v0x556a177820_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x556a177570_0, 0;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x556a1239d0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556a177eb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556a177ce0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556a177de0_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x556a1239d0;
T_4 ;
    %vpi_call 2 48 "$display", "Starting SPI RX Test" {0 0 0};
    %vpi_call 2 49 "$dumpfile", "spi_rx_tb.vcd" {0 0 0};
    %vpi_call 2 50 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x556a1239d0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556a177ce0_0, 0, 1;
    %delay 40000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556a177ce0_0, 0, 1;
    %pushi/vec4 165, 0, 8;
    %store/vec4 v0x556a123de0_0, 0, 8;
    %fork TD_spi_rx_tb.send_spi_byte, S_0x556a123c00;
    %join;
    %delay 40000, 0;
    %pushi/vec4 179, 0, 8;
    %store/vec4 v0x556a123de0_0, 0, 8;
    %fork TD_spi_rx_tb.send_spi_byte, S_0x556a123c00;
    %join;
    %delay 40000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556a177ce0_0, 0, 1;
    %delay 40000, 0;
    %vpi_call 2 67 "$display", "Test complete" {0 0 0};
    %vpi_call 2 68 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "spi_rx_tb.v";
    "../src/periph/spi_rx.v";
