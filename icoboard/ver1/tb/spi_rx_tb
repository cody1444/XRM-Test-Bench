#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/va_math.vpi";
S_0x5596ed5ab0 .scope module, "spi_rx_tb" "spi_rx_tb" 2 3;
 .timescale -9 -12;
P_0x5596ed5c40 .param/l "WIDTH" 0 2 6, +C4<00000000000000000000000000001000>;
v0x5596eee6b0_0 .net "byte_ready", 0 0, v0x5596eee0f0_0;  1 drivers
v0x5596eee770_0 .net "command_byte", 7 0, L_0x5596eeeb00;  1 drivers
v0x5596eee840_0 .var "cs", 0 0;
v0x5596eee940_0 .var "mosi", 0 0;
v0x5596eeea10_0 .var "sclk", 0 0;
S_0x5596ed5ce0 .scope task, "send_spi_byte" "send_spi_byte" 2 27, 2 27 0, S_0x5596ed5ab0;
 .timescale -9 -12;
v0x5596ed5ec0_0 .var "data", 7 0;
v0x5596eedb50_0 .var/i "i", 31 0;
TD_spi_rx_tb.send_spi_byte ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5596eee840_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 7, 0, 32;
    %store/vec4 v0x5596eedb50_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5596eedb50_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5596eeea10_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x5596ed5ec0_0;
    %load/vec4 v0x5596eedb50_0;
    %part/s 1;
    %store/vec4 v0x5596eee940_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5596eeea10_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x5596eedb50_0;
    %subi 1, 0, 32;
    %store/vec4 v0x5596eedb50_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5596eeea10_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5596eee840_0, 0, 1;
    %delay 20000, 0;
    %end;
S_0x5596eedc30 .scope module, "uut" "spi_rx" 2 18, 3 1 0, S_0x5596ed5ab0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "sclk";
    .port_info 1 /INPUT 1 "cs";
    .port_info 2 /INPUT 1 "mosi";
    .port_info 3 /OUTPUT 8 "command_byte";
    .port_info 4 /OUTPUT 1 "byte_ready";
P_0x5596eede30 .param/l "WIDTH" 0 3 2, +C4<00000000000000000000000000001000>;
L_0x5596eeeb00 .functor BUFZ 8, v0x5596eee530_0, C4<00000000>, C4<00000000>, C4<00000000>;
v0x5596eedff0_0 .var "bit_count", 2 0;
v0x5596eee0f0_0 .var "byte_ready", 0 0;
v0x5596eee1b0_0 .net "command_byte", 7 0, L_0x5596eeeb00;  alias, 1 drivers
v0x5596eee2a0_0 .net "cs", 0 0, v0x5596eee840_0;  1 drivers
v0x5596eee360_0 .net "mosi", 0 0, v0x5596eee940_0;  1 drivers
v0x5596eee470_0 .net "sclk", 0 0, v0x5596eeea10_0;  1 drivers
v0x5596eee530_0 .var "shift_reg", 7 0;
E_0x5596ed2c70 .event posedge, v0x5596eee2a0_0;
E_0x5596e9a450 .event negedge, v0x5596eee470_0;
    .scope S_0x5596eedc30;
T_1 ;
    %wait E_0x5596e9a450;
    %load/vec4 v0x5596eee530_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x5596eee360_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5596eee530_0, 0;
    %load/vec4 v0x5596eedff0_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x5596eedff0_0, 0;
    %load/vec4 v0x5596eedff0_0;
    %pad/u 32;
    %cmpi/e 7, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5596eee0f0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5596eee0f0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5596eedc30;
T_2 ;
    %wait E_0x5596ed2c70;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5596eee530_0, 0;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5596eedff0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5596eee0f0_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5596ed5ab0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5596eeea10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5596eee840_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5596eee940_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x5596ed5ab0;
T_4 ;
    %vpi_call 2 51 "$display", "Starting SPI RX Test" {0 0 0};
    %vpi_call 2 52 "$dumpfile", "spi_rx_tb.vcd" {0 0 0};
    %vpi_call 2 53 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5596ed5ab0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5596eee840_0, 0, 1;
    %delay 40000, 0;
    %pushi/vec4 165, 0, 8;
    %store/vec4 v0x5596ed5ec0_0, 0, 8;
    %fork TD_spi_rx_tb.send_spi_byte, S_0x5596ed5ce0;
    %join;
    %delay 40000, 0;
    %pushi/vec4 179, 0, 8;
    %store/vec4 v0x5596ed5ec0_0, 0, 8;
    %fork TD_spi_rx_tb.send_spi_byte, S_0x5596ed5ce0;
    %join;
    %delay 40000, 0;
    %vpi_call 2 66 "$display", "Test complete" {0 0 0};
    %vpi_call 2 67 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "spi_rx_tb.v";
    "spi_rx.v";
