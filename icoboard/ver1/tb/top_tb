#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/va_math.vpi";
S_0x559a91a830 .scope module, "top_tb" "top_tb" 2 3;
 .timescale -9 -12;
v0x559a939560_0 .var "CLK", 0 0;
v0x559a939670_0 .var "CS", 0 0;
v0x559a939780_0 .var "MOSI", 0 0;
v0x559a939870_0 .var "SCLK", 0 0;
v0x559a939960_0 .net "pins", 63 0, v0x559a938110_0;  1 drivers
S_0x559a91a9c0 .scope task, "send_spi_byte" "send_spi_byte" 2 28, 2 28 0, S_0x559a91a830;
 .timescale -9 -12;
v0x559a9183c0_0 .var "data", 7 0;
v0x559a9374a0_0 .var/i "i", 31 0;
TD_top_tb.send_spi_byte ;
    %delay 5000, 0;
    %pushi/vec4 7, 0, 32;
    %store/vec4 v0x559a9374a0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x559a9374a0_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559a939870_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x559a9183c0_0;
    %load/vec4 v0x559a9374a0_0;
    %part/s 1;
    %store/vec4 v0x559a939780_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559a939870_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x559a9374a0_0;
    %subi 1, 0, 32;
    %store/vec4 v0x559a9374a0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559a939870_0, 0, 1;
    %delay 40000, 0;
    %end;
S_0x559a937580 .scope module, "uut" "top" 2 15, 3 1 0, S_0x559a91a830;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "CLK";
    .port_info 1 /INPUT 1 "SCLK";
    .port_info 2 /INPUT 1 "CS";
    .port_info 3 /INPUT 1 "MOSI";
    .port_info 4 /OUTPUT 64 "pins";
v0x559a938e80_0 .net "BYTE_READY", 0 0, v0x559a9389b0_0;  1 drivers
v0x559a938f40_0 .net "CLK", 0 0, v0x559a939560_0;  1 drivers
v0x559a939000_0 .net "COMMAND_BYTE", 7 0, v0x559a938ac0_0;  1 drivers
v0x559a939120_0 .net "CS", 0 0, v0x559a939670_0;  1 drivers
v0x559a9391c0_0 .net "MOSI", 0 0, v0x559a939780_0;  1 drivers
v0x559a9392b0_0 .net "SCLK", 0 0, v0x559a939870_0;  1 drivers
v0x559a939380_0 .net "TOGGLE_MASK", 63 0, v0x559a937c50_0;  1 drivers
v0x559a939470_0 .net "pins", 63 0, v0x559a938110_0;  alias, 1 drivers
S_0x559a937800 .scope module, "decode" "decoder" 3 23, 4 1 0, S_0x559a937580;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "byte_ready";
    .port_info 1 /INPUT 8 "command_byte";
    .port_info 2 /OUTPUT 64 "toggle_mask";
v0x559a937a90_0 .net "byte_ready", 0 0, v0x559a9389b0_0;  alias, 1 drivers
v0x559a937b70_0 .net "command_byte", 7 0, v0x559a938ac0_0;  alias, 1 drivers
v0x559a937c50_0 .var "toggle_mask", 63 0;
E_0x559a919400 .event posedge, v0x559a937a90_0;
S_0x559a937d90 .scope module, "pin_ctrl" "pin_controller" 3 30, 5 1 0, S_0x559a937580;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 64 "toggle_mask";
    .port_info 2 /INPUT 1 "toggle_enable";
    .port_info 3 /OUTPUT 64 "output_pins";
v0x559a938030_0 .net "clk", 0 0, v0x559a939560_0;  alias, 1 drivers
v0x559a938110_0 .var "output_pins", 63 0;
v0x559a9381f0_0 .var "prev_toggle_enable", 0 0;
v0x559a9382c0_0 .net "toggle_enable", 0 0, v0x559a9389b0_0;  alias, 1 drivers
v0x559a938390_0 .net "toggle_mask", 63 0, v0x559a937c50_0;  alias, 1 drivers
E_0x559a919880 .event posedge, v0x559a938030_0;
S_0x559a938510 .scope module, "spi_receiver" "spi_rx" 3 14, 6 1 0, S_0x559a937580;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "sclk";
    .port_info 1 /INPUT 1 "cs";
    .port_info 2 /INPUT 1 "mosi";
    .port_info 3 /OUTPUT 8 "command_byte";
    .port_info 4 /OUTPUT 1 "byte_ready";
P_0x559a9386f0 .param/l "WIDTH" 0 6 2, +C4<00000000000000000000000000001000>;
v0x559a9388b0_0 .var "bit_count", 2 0;
v0x559a9389b0_0 .var "byte_ready", 0 0;
v0x559a938ac0_0 .var "command_byte", 7 0;
v0x559a938b90_0 .net "cs", 0 0, v0x559a939670_0;  alias, 1 drivers
v0x559a938c30_0 .net "mosi", 0 0, v0x559a939780_0;  alias, 1 drivers
v0x559a938d20_0 .net "sclk", 0 0, v0x559a939870_0;  alias, 1 drivers
E_0x559a8fb9f0 .event negedge, v0x559a938b90_0;
E_0x559a907280 .event negedge, v0x559a938d20_0;
    .scope S_0x559a938510;
T_1 ;
    %wait E_0x559a907280;
    %load/vec4 v0x559a938ac0_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x559a938c30_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x559a938ac0_0, 0;
    %load/vec4 v0x559a9388b0_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x559a9388b0_0, 0;
    %load/vec4 v0x559a9388b0_0;
    %pad/u 32;
    %cmpi/e 7, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x559a9389b0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x559a9389b0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x559a938510;
T_2 ;
    %wait E_0x559a8fb9f0;
    %load/vec4 v0x559a938b90_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x559a9388b0_0, 0;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x559a937800;
T_3 ;
    %wait E_0x559a919400;
    %pushi/vec4 1, 0, 64;
    %load/vec4 v0x559a937b70_0;
    %parti/s 6, 0, 2;
    %ix/vec4 4;
    %shiftl 4;
    %assign/vec4 v0x559a937c50_0, 0;
    %pushi/vec4 1, 0, 64;
    %load/vec4 v0x559a937b70_0;
    %parti/s 6, 0, 2;
    %ix/vec4 4;
    %shiftl 4;
    %vpi_call 4 9 "$display", "decoder: command_byte = %h, toggle_mask = %b", v0x559a937b70_0, S<0,vec4,u64> {1 0 0};
    %jmp T_3;
    .thread T_3;
    .scope S_0x559a937d90;
T_4 ;
    %pushi/vec4 0, 0, 64;
    %store/vec4 v0x559a938110_0, 0, 64;
    %end;
    .thread T_4;
    .scope S_0x559a937d90;
T_5 ;
    %wait E_0x559a919880;
    %load/vec4 v0x559a9381f0_0;
    %inv;
    %load/vec4 v0x559a9382c0_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.0, 8;
    %load/vec4 v0x559a938110_0;
    %load/vec4 v0x559a938390_0;
    %xor;
    %assign/vec4 v0x559a938110_0, 0;
    %load/vec4 v0x559a938110_0;
    %load/vec4 v0x559a938390_0;
    %xor;
    %vpi_call 5 14 "$display", "pin_controller: Toggling output_pins <= %b", S<0,vec4,u64> {1 0 0};
T_5.0 ;
    %load/vec4 v0x559a9382c0_0;
    %assign/vec4 v0x559a9381f0_0, 0;
    %jmp T_5;
    .thread T_5;
    .scope S_0x559a91a830;
T_6 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559a939560_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559a939870_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559a939670_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559a939780_0, 0, 1;
    %end;
    .thread T_6;
    .scope S_0x559a91a830;
T_7 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559a939560_0, 0, 1;
    %end;
    .thread T_7;
    .scope S_0x559a91a830;
T_8 ;
    %delay 5000, 0;
    %load/vec4 v0x559a939560_0;
    %inv;
    %store/vec4 v0x559a939560_0, 0, 1;
    %jmp T_8;
    .thread T_8;
    .scope S_0x559a91a830;
T_9 ;
    %vpi_call 2 48 "$display", "Starting top module testbench..." {0 0 0};
    %vpi_call 2 49 "$dumpfile", "top_tb.vcd" {0 0 0};
    %vpi_call 2 50 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x559a91a830 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559a939670_0, 0, 1;
    %delay 40000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559a939670_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x559a9183c0_0, 0, 8;
    %fork TD_top_tb.send_spi_byte, S_0x559a91a9c0;
    %join;
    %delay 40000, 0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x559a9183c0_0, 0, 8;
    %fork TD_top_tb.send_spi_byte, S_0x559a91a9c0;
    %join;
    %delay 40000, 0;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x559a9183c0_0, 0, 8;
    %fork TD_top_tb.send_spi_byte, S_0x559a91a9c0;
    %join;
    %delay 40000, 0;
    %pushi/vec4 63, 0, 8;
    %store/vec4 v0x559a9183c0_0, 0, 8;
    %fork TD_top_tb.send_spi_byte, S_0x559a91a9c0;
    %join;
    %delay 40000, 0;
    %pushi/vec4 63, 0, 8;
    %store/vec4 v0x559a9183c0_0, 0, 8;
    %fork TD_top_tb.send_spi_byte, S_0x559a91a9c0;
    %join;
    %delay 1000000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559a939670_0, 0, 1;
    %delay 40000, 0;
    %vpi_call 2 73 "$display", "Final pins = %b", v0x559a939960_0 {0 0 0};
    %vpi_call 2 74 "$finish" {0 0 0};
    %end;
    .thread T_9;
